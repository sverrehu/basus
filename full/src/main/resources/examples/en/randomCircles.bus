repeat 100 times

    // Select a random color (red, green, blue).
    color(random(255), random(255), random(255));

    x = random(width() - 1);
    y = random(height() - 1);
    full_radius = random(10, 100);

    for step_radius = 0 to full_radius do
        fillCircle(x, y, step_radius);
    done;

done;
